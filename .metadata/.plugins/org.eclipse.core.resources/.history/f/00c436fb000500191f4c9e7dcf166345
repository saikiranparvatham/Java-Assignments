import static org.junit.Assert.*;

import org.junit.Test;


public class TestJavaGrep {
	JavaGrep newObject = new JavaGrep();
	
	@Test
	public void testPatternMatch() throws Exception {
		
		int[] lineNumber = newObject.patternMatch("C:/New folder/New Text Document.txt","line2");
		assertEquals(2,lineNumber);
	}
	
	

}
