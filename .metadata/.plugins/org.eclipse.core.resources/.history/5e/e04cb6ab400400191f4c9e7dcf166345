public class Training {
	
	private int Id;
	private String subject;
	protected static double fees;


	public Training(int id,String subject,double fees)
	{
		this.Id = id;
		this.subject = subject;
		this.fees = fees;
	}
	
	public int getId()
	{
		return this.Id;
	}
	
	public double getFees()
	{
		return this.fees;
	}
}
