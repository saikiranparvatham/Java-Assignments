
public class Country {
	public String nameOfCountry;
	public long population;
	public long area;
	
	public Country(String nameOfCountry, long population, long area) {
		this.nameOfCountry = nameOfCountry;
		this.population = population;
		this.area = area;
	}
	
	public static String findLargestArea(Country country1,Country country2,Country country3,Country country4,Country country5)
	{
		System.out.println(country1.area);
		return "str";
	}
	
	
}
