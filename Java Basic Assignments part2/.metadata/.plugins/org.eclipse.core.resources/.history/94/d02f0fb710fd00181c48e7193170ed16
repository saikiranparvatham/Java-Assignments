
public class reverseWords {
	public String findReverse(String input)
	{
		
		 String[] words = input.split(" ");
	        String reverse = "";
	        for (int i = 0; i < words.length; i++) 
			{
	            for (int j = words[i].length() - 1; j >= 0; j--) 
			{
	                reverse += words[i].charAt(j);
	            	}
	            System.out.print(reverse + " ");
	            reverse = "";
	        	}
		return reverse;
	}
}
